<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="bocai.com.yanghuaji">

    <application
        android:name=".base.Application"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ui.account.LoginActivity"
            android:theme="@style/BaseTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.account.BindPhoneActivity"
            android:theme="@style/BaseTheme" />
        <activity
            android:name=".ui.account.RegisterActivity"
            android:theme="@style/BaseTheme" />
        <activity
            android:name=".ui.account.ForgetPasswordActivity"
            android:theme="@style/BaseTheme" />
        <activity
            android:name=".ui.main.MainActivity"
            android:theme="@style/BaseTheme" />
        <activity
            android:name=".ui.personalCenter.EditPersonalDataActivity"
            android:theme="@style/BaseTheme" />
        <activity
            android:name=".ui.personalCenter.ModifyPasswordActivity"
            android:theme="@style/BaseTheme" />
        <activity
            android:name=".ui.personalCenter.PlantingRecordActivity"
            android:theme="@style/BaseTheme" />
        <activity
            android:name=".ui.personalCenter.SystemNotificationActivity"
            android:theme="@style/BaseTheme" />
        <activity
            android:name=".ui.personalCenter.SettingActivity"
            android:theme="@style/BaseTheme" />


    </application>

</manifest>