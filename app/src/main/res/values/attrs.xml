<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RoundAngleImageView">
        <attr name="roundWidth" format="dimension" />
        <attr name="roundHeight" format="dimension" />
        <attr name="roundHeightLeftUp" format="dimension" />
        <attr name="roundHeightRightUp" format="dimension" />
        <attr name="roundHeightLeftBottom" format="dimension" />
        <attr name="roundHeightRightBottom" format="dimension" />
    </declare-styleable>


    -
    <declare-styleable name="DashView">
        <attr name="dashWidth" format="dimension" />
        <attr name="lineWidth" format="dimension" />
        <attr name="lineHeight" format="dimension" />
        <attr name="lineColor" format="color" />
        <attr name="dashOrientation" format="integer" />
    </declare-styleable>

    <declare-styleable name="WaterRippleView">
        <attr name="rippleSpacing" format="dimension|reference" />
        <attr name="rippleCount" format="integer|reference" />
        <attr name="waterRippleColor" format="color|reference" />
        <attr name="rippleCenterIcon" format="reference" />
        <attr name="rippleAutoRunning" format="boolean" />
    </declare-styleable>

</resources>